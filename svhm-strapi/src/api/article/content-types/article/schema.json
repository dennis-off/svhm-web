{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Articles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "blocks"
    },
    "dynamic_zone": {
      "type": "dynamiczone",
      "components": [
        "dynamic-zone.related-articles"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member"
    }
  }
}
